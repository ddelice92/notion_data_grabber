{
    "manifest_version": 3,
    "name": "Grabber",
    "description": "grab notion data",
    "version": "1.0",

    "content_scripts": [
        {
            "matches": ["https://*/*"],
            "js": ["background.js","jquery-3.7.1.min.js"],
            "run_at": "document_idle"
        }
    ],
    "permissions": ["activeTab", "scripting"],
    "action": {
        "default_icon": "grabber.png"
    }
}